{
    "deviceOrientation": "portrait",
    "iOSHighPerformance": true,
    "showStatusBar": false,
    "networkTimeout": {
        "request": 5000,
        "connectSocket": 5000,
        "uploadFile": 5000,
        "downloadFile": 5000
    },
    "workers": "workers",
    "subpackages": [
        {
            "name": "api",
            "root": "/js/api/"
        }
    ],
    "permission": {
        "scope.userLocation": {
            "desc": "你的位置信息将用于小程序位置接口的效果展示"
        },
        "scope.userInfo": {
            "desc": "您的信息将用于展示小游戏用户信息效果展示"
        }
    }
}
